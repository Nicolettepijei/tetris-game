{
  "name": "mimstris",
  "version": "0.0.1",
  "description": "A challenging puzzle game, just for funsies",
  "main": "main.js",
  "repository": {
    "url": "https://github.com/mimshwright/mimstris",
    "type": "git"
  },
  "author": "Mims H. Wright",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4",
    "pressed": "^1.2.0",
    "tape": "^4.6.3"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6",
    "webpack": "^1.14.0"
  },
  "scripts": {
    "lint": "standard './*.js' './src/**/*.js'",
    "fix": "standard --fix './*.js' './src/**/*.js'",
    "test": "tape -r babel-register test/**/*.test.js | node_modules/.bin/tap-spec",
    "build": "webpack --config webpack.config.babel.js",
    "watch": "webpack --watch --config webpack.config.babel.js",
    "start": "npm run fix && npm run test && npm run watch && open dist/index.html"
  }
}
